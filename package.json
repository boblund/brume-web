{
  "name": "brume-web",
  "version": "1.0.0",
  "description": "Brume web files",
  "main": "brumeLogin.mjs",
  "scripts": {
    "mv_cognito": "mv -f node_modules/amazon-cognito-identity-js/dist/amazon-cognito-identity.min.js amazon-cognito-identity.min.js",
    "mv_brumeLogin": "mv -f node_modules/brume-web/brumeLogin.mjs brumeLogin.mjs",
		"mv_brume-elements": "mv -f node_modules/brume-web/brume-elements.mjs brume-elements.mjs",
    "rm-node_modules": "rm -rf node_modules",
    "postinstall": "npm run mv_cognito && npm run mv_brumeLogin && npm run mv_brume-elements && npm run rm-node_modules",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Bob Lund",
  "license": "CC-BY-NC-4.0",
  "dependencies": {
    "amazon-cognito-identity-js": "^6.3.12"
  }
}
